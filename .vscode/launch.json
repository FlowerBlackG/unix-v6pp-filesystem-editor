{
    "configurations": [
        {
            "name": "gdb debug fsedit.exe",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\output\\fsedit.exe",
            "args": [
                "${workspaceFolder}\\output\\c-back.img", // 磁盘文件路径。
                "e"
            ],
            "stopAtEntry": false,
            "cwd": "C:/devkits/TDM-GCC-64/bin",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\devkits\\TDM-GCC-64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": ""
        }
    ],
    "version": "2.0.0"
}